<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.com.ibm.hackthon.dao.BehaviorMapper">
  <resultMap id="BaseResultMap" type="cn.com.ibm.hackthon.po.Behavior">
    <result column="userid" jdbcType="INTEGER" property="userid" />
    <result column="itemid" jdbcType="INTEGER" property="itemid" />
    <result column="counter" jdbcType="INTEGER" property="counter" />
  </resultMap>
  <insert id="insert" parameterType="cn.com.ibm.hackthon.po.Behavior">
    insert into t_beha (userid, itemid, counter
      )
    values (#{userid,jdbcType=INTEGER}, #{itemid,jdbcType=INTEGER}, #{counter,jdbcType=INTEGER}
      )
  </insert>
  <insert id="insertSelective" parameterType="cn.com.ibm.hackthon.po.Behavior">
    insert into t_beha
    <trim prefix="(" suffix=")" suffixOverrides=",">
      <if test="userid != null">
        userid,
      </if>
      <if test="itemid != null">
        itemid,
      </if>
      <if test="counter != null">
        counter,
      </if>
    </trim>
    <trim prefix="values (" suffix=")" suffixOverrides=",">
      <if test="userid != null">
        #{userid,jdbcType=INTEGER},
      </if>
      <if test="itemid != null">
        #{itemid,jdbcType=INTEGER},
      </if>
      <if test="counter != null">
        #{counter,jdbcType=INTEGER},
      </if>
    </trim>
  </insert>
</mapper>